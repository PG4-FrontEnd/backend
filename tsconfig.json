{
	"compilerOptions": {
	  "target": "ES6",
	  "lib": ["ES6", "DOM"],
	  "module": "CommonJS",
	  "allowJs": true,
	  "checkJs": true,
	  "noImplicitAny": false,
	  "strict": true,
	  "esModuleInterop": true,
	  "moduleResolution": "Node",
	  "forceConsistentCasingInFileNames": true
	},
	"include": ["./src/**/*"],
	"exclude": ["node_modules"]
  }